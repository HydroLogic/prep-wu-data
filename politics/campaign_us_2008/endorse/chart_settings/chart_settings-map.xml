<?xml version="1.0" encoding="utf-8"?>
<settings> 
  <!-- Map Overlay -->
  <!-- <background> <file>pix/google_map.jpg</file> <alpha>60</alpha>  </background> -->
  <background> <file>pix/simple_map.png</file> <alpha>60</alpha>  </background>

  <!-- Geometry -->
  <grid>
    <x> <approx_count>100</approx_count> <alpha>0</alpha> </x>
    <y> <approx_count>100</approx_count> <alpha>0</alpha> </y>
  </grid>
  <values>
    <x> <min>-126</min> <max>-66</max>  <strict_min_max>true</strict_min_max> </x>
    <y> <min>24</min>   <max>50</max>   <strict_min_max>true</strict_min_max> </y>
  </values>
  <plot_area>
    <color>ddddee</color> <alpha>0</alpha>
    <margins> <left>4</left> <right>-19</right> <top>6</top> <bottom>-5</bottom> </margins>
  </plot_area>
  
  <graphs> 
    <graph gid="0" >
      <!-- No Line -->
      <alpha>0</alpha> <color>1111ff</color> <title>Hi!</title>
      <!-- Bubbles -->
      <bullet>bubble</bullet> 
      <bullet_max_size>150</bullet_max_size>
      <bullet_min_size>10</bullet_min_size>
      <!-- Popups -->       
      <balloon_text>{description}</balloon_text> 
      <bullet_alpha>60</bullet_alpha>
    </graph>
    <graph gid="1" >
      <!-- No Line --> <alpha>0</alpha> <color>1111ff</color>
      <bullet>bubble</bullet> <bullet_max_size>16</bullet_max_size>
      <bullet_alpha>90</bullet_alpha> <balloon_text>{description}</balloon_text> 
    </graph>    
    <graph gid="2" >
      <!-- No Line --> <alpha>0</alpha> <color>1111ff</color>
      <bullet>round</bullet> <bullet_max_size>150</bullet_max_size>
      <bullet_alpha>30</bullet_alpha> <bullet_color>333333</bullet_color>
      <data_labels>{description}</data_labels>
    </graph>    
  </graphs>
  
  <labels>
    <label> <x>!146</x>   <y>!152</y> <text>Circulation</text> <align>center</align> <text_size>12</text_size> <text_color>777777</text_color> </label>


<label> <x>!131</x> <y>!105</y> <text>Bush in '04</text> <align>left</align> <text_size>13</text_size> <text_color>444444</text_color> </label>
<label> <x>!131</x> <y>!85</y> <text>Kerry or none in '04</text> <align>left</align> <text_size>13</text_size> <text_color>444444</text_color> </label>
<label> <x>!131</x> <y>!40</y> <text>Bush or none in '04</text> <align>left</align> <text_size>13</text_size> <text_color>444444</text_color> </label>
<label> <x>!131</x> <y>!20</y> <text>Kerry in '04</text> <align>left</align> <text_size>13</text_size> <text_color>444444</text_color> </label>
<label> <x>!155</x> <y>!122</y> <text>Obama (135/~15.1M tot)</text> <align>left</align> <text_size>13</text_size> <text_color>444444</text_color> </label>
<label> <x>!155</x> <y>!57</y> <text>McCain (52/~4.1M tot)</text> <align>left</align> <text_size>13</text_size> <text_color>444444</text_color> </label>

<!--  (Bush '04) 17/~2.7M tot. ) 27/~3.0M -->

  </labels>

  <bullets>   <hover_brightness>-32</hover_brightness>      </bullets>
  <balloon>
    <border_width>1</border_width> 
    <border_alpha>50</border_alpha>
    <border_color>180808</border_color>
    <color>444444</color>
    <alpha>75</alpha>
  </balloon>
  <!-- clean up graph appearance -->
  <legend>    <enabled>false</enabled> <x>30</x> <y>!30</y> </legend>
  <axes><x> <alpha>0</alpha> </x> <y> <alpha>0</alpha> </y> </axes>
  <zoom>  <target>false</target> </zoom>
  <decimals_separator>.</decimals_separator>
  <thousands_separator>,</thousands_separator>
  <font>Trebuchet MS, sans-serif</font>
  <text_color>ffffff</text_color>
  <!-- check for updated data -->
  <add_time_stamp>true</add_time_stamp>
</settings>
