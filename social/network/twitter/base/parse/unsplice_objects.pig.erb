register <%= piggybank_jar %>;

api_objects    = LOAD '<%= api %>';
stream_objects = LOAD '<%= stream %>';

objects_to_unsplice = UNION api_objects, stream_objects;
STORE objects_to_unsplice INTO '<%= hdfs %>/<%= out %>' USING org.apache.pig.piggybank.storage.MultiStorage('<%= out %>','0', 'none', '\\t');
